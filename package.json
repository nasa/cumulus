{
  "name": "gibs-dashboard",
  "version": "0.1.0",
  "description": "A dashboard for controlling GIBS in AWS.",
  "repository": {
    "type": "git",
    "url": "https://git.earthdata.nasa.gov/projects/GITC/repos/gibs-dashboard"
  },
  "author": {
    "name": "NASA EOSDIS",
    "url": "https://earthdata.nasa.gov/"
  },
  "homepage": "https://git.earthdata.nasa.gov/projects/GITC/repos/gibs-dashboard/browse",
  "scripts": {
    "postinstall": "[ -f app/scripts/config/local.js ] || echo 'module.exports = {};' > app/scripts/config/local.js",
    "serve": "GIBS_ENV=development gulp serve",
    "staging": "GIBS_ENV=staging gulp",
    "production": "NODE_ENV=production GIBS_ENV=production gulp",
    "lint": "eslint app/scripts/ --ext .js",
    "start": "npm run serve",
    "mocha": "mocha --require babel-polyfill --compilers js:babel-core/register --recursive",
    "test": "npm run lint && npm run mocha"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ],
      "require-globify",
      "envify"
    ]
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-eslint": "^7.2.2",
    "babel-plugin-extensible-destructuring": "^4.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.23.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.12.5",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "del": "^2.2.0",
    "envify": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-cache": "^0.4.4",
    "gulp-csso": "^2.0.0",
    "gulp-exit": "0.0.2",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.1",
    "gulp-load-plugins": "^1.2.2",
    "gulp-plumber": "^1.1.0",
    "gulp-preprocess": "^2.0.0",
    "gulp-rev": "^7.0.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^2.3.1",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-useref": "^3.1.0",
    "gulp-util": "^3.0.7",
    "jsx-chai": "^4.0.0",
    "mocha": "^3.2.0",
    "node-bourbon": "^4.2.8",
    "node-notifier": "^4.5.0",
    "node-sass": "^3.7.0",
    "require-globify": "^1.3.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "babel-polyfill": "^6.23.0",
    "connect-history-api-fallback": "^1.3.0",
    "d3-queue": "^3.0.5",
    "d3-scale": "^1.0.5",
    "deepmerge": "^1.3.2",
