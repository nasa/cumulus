    "javascript-time-ago": "^0.4.9",
    "lodash.debounce": "^4.0.8",
    "lodash.findkey": "^4.6.0",
    "lodash.map": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.17.1",
    "numeral": "^2.0.4",
    "object-path": "^0.11.3",
    "paginator": "^1.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-functional": "^2.0.0",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
