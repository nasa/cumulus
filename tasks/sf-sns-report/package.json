{
  "name": "@cumulus/sf-sns-report",
  "version": "1.16.1",
  "description": "Broadcasts an incoming Cumulus message to SNS",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "homepage": "https://github.com/nasa/cumulus/tree/master/tasks/sf-sns-report",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "test": "ava",
    "test-coverage": "nyc ava",
    "build": "rm -rf dist && mkdir dist && webpack",
    "build-lambda-zips": "(cd dist && rm -f lambda.zip && zip lambda.zip index.js)",
    "watch": "rm -rf dist && mkdir dist && webpack --progress -w",
    "prepare": "npm run build && npm run build-lambda-zips"
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/api": "1.16.1",
    "@cumulus/cumulus-message-adapter-js": "^1.0.10",
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@cumulus/common": "1.16.1",
    "ava": "^2.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-source-map-support": "^2.1.1",
    "babel-preset-env": "^1.7.0",
    "lodash.clonedeep": "^4.5.0",
    "nyc": "^14.0.0",
    "webpack": "~4.5.0",
    "webpack-cli": "~2.0.14"
  }
}
