{
  "title": "MoveGranulesOutput",
  "description": "Describes the output produced by the move-granules task",
  "type": "object",
  "properties": {
    "granules": {
      "type": "array",
      "description": "Array of all granules",
      "items": {
        "type": "object",
        "required": ["granuleId", "files"],
        "properties": {
          "granuleId": {
            "type": "string"
          },
          "files": {
            "type": "array",
            "items": {
              "type": "object",
              "required": ["name"],
              "properties": {
                "name": {
                  "type": "string"
                },
                "filename": {
                  "type": "string",
                  "description": "The s3 bucket/path URI (e.g. s3://{bucket}{object})"
                },
                "filepath": {
                  "type": "string",
                  "description": "The S3 object key"
                },
                "type": {
                  "type": "string"
                },
                "bucket": {
                  "type": "string"
                },
                "etag": {
                  "description": "Entity tag for the current version of the file",
                  "type": "string"
                },
                "duplicate_found": {
                  "description": "whether a duplicate for this file was found",
                  "type": "boolean"
                }
              }
            }
          }
        }
      }
    }
  }
}
