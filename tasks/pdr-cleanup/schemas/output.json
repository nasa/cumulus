{
    "title": "PdrCleanupOutput",
    "description": "Describes the output produced by the pdr-cleanup task",
    "type": "object",
    "required": ["pdr"],
    "properties": {
      "pdr": {
        "description": "Product Delivery Record",
        "type": "object",
        "required": ["archivePath"],
        "properties": {
          "name": { "type": "string" },
          "path": { "type": "string" },
          "archivePath": { "type": "string" }
        }
      },
      "running": {
        "description": "List of execution arns which are queued or running",
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "completed": {
        "description": "List of completed execution arns",
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "failed": {
        "description": "List of failed execution arns with reason",
        "type": "array",
        "items": {
          "type": "object",
          "required": ["arn", "reason"],
          "properties": {
            "arn": { "type": "string" },
            "reason": { "type": "string" }
          }
        }
      }
    }
  }
