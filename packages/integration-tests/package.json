{
  "name": "@cumulus/integration-tests",
  "version": "2.0.0",
  "description": "Integration tests",
  "bin": {
    "cumulus-test": "./bin/cli.js"
  },
  "engines": {
    "node": ">=12.18.0"
  },
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/nasa/cumulus/tree/master/packages/integration-tests/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus"
  },
  "scripts": {
    "build": "rm -rf dist && mkdir dist && ../../node_modules/.bin/webpack",
    "build-docs": "../../node_modules/.bin/jsdoc2md --template README.hbs Collections.js Executions.js Granules.js Providers.js Rules.js > README.md",
    "package": "npm run build",
    "test": "../../node_modules/.bin/ava",
    "test:coverage": "../../node_modules/.bin/nyc npm test"
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/api": "2.0.0",
    "@cumulus/api-client": "2.0.0",
    "@cumulus/aws-client": "2.0.0",
    "@cumulus/cmrjs": "2.0.0",
    "@cumulus/common": "2.0.0",
    "@cumulus/earthdata-login-client": "2.0.0",
    "@cumulus/launchpad-auth": "2.0.0",
    "@cumulus/message": "2.0.0",
    "aws-sdk": "^2.585.0",
    "base-64": "^0.1.0",
    "commander": "^2.15.0",
    "delay": "^4.3.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^5.0.0",
    "got": "^8.3.0",
    "handlebars": "^4.0.11",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "2.27.0",
    "p-map": "^2.1.0",
    "p-retry": "^2.0.0",
    "p-wait-for": "^2.0.1",
    "uuid": "^3.2.1",
    "xml2js": "^0.4.19"
  }
}
