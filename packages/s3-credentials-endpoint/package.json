{
  "name": "@cumulus/s3-credentials-endpoint",
  "version": "5.0.0",
  "description": "An API Gateway Lambda to return AWS credentials for fetching objects from S3",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12.18.0"
  },
  "scripts": {
    "build": "../../node_modules/.bin/webpack && (cd dist && rm -f lambda.zip && node ../../../bin/zip.js lambda.zip index.js instructions)",
