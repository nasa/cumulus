{
  "name": "@cumulus/tf-inventory",
  "version": "2.0.0",
  "description": "Package to help keep track of what resources are managed by Terraform in the AWS account",
  "main": "index.js",
  "engines": {
    "node": ">=12.18.0"
  },
  "bin": {
    "tf-inventory": "./bin/cli.js"
  },
  "scripts": {
    "test": "../../node_modules/.bin/ava",
    "test:coverage": "../../node_modules/.bin/nyc npm test"
  },
  "ava": {
    "files": [
      "tests/**"
    ],
    "fail-fast": true,
    "verbose": true,
    "timeout": "15m"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Cumulus"
  ],
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/aws-client": "2.0.0",
    "commander": "^4.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@cumulus/common": "2.0.0"
  }
}
