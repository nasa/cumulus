{
  "name": "@cumulus/ingest",
  "version": "2.0.0",
  "description": "Ingest utilities",
  "engines": {
    "node": ">=12.18.0"
  },
  "scripts": {
    "clean": "rm -f *.js *.map *.d.ts",
    "prepare": "npm run tsc",
    "test": "../../node_modules/.bin/ava",
    "test:coverage": "../../node_modules/.bin/nyc npm test",
    "tsc": "../../node_modules/.bin/tsc",
    "watch-test": "../../node_modules/.bin/tsc-watch --onsuccess 'npm test'"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "!test/fixtures/**/*"
    ],
    "timeout": "15m"
  },
  "keywords": [
    "GIBS",
    "CUMULUS",
    "NASA"
  ],
  "homepage": "https://github.com/nasa/cumulus/tree/master/packages/ingest#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus",
    "directory": "packages/ingest"
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/aws-client": "2.0.0",
    "@cumulus/common": "2.0.0",
    "@cumulus/errors": "2.0.0",
    "@cumulus/message": "2.0.0",
    "@cumulus/sftp-client": "2.0.0",
    "aws-sdk": "^2.585.0",
    "cksum": "^1.3.0",
    "delay": "^4.3.0",
    "encodeurl": "^1.0.2",
    "fs-extra": "^5.0.0",
    "got": "^9.2.1",
    "is-ip": "^2.0.0",
    "is-valid-hostname": "^0.0.1",
    "jsftp": "https://github.com/jkovarik/jsftp.git#add_288",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.22",
    "moment": "2.24.0",
    "simplecrawler": "^1.1.9",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@cumulus/checksum": "2.0.0",
    "@cumulus/cmrjs": "2.0.0",
    "@cumulus/test-data": "2.0.0"
  }
}
