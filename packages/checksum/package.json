{
  "name": "@cumulus/checksum",
  "version": "19.0.0",
  "description": "Cumulus checksum utilities",
  "engines": {
    "node": ">=20.12.2"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build-docs": "npm run tsc && ../../node_modules/.bin/jsdoc2md -t templates/README.md.hbs ./dist/*.js > README.md",
    "clean": "rm -rf dist",
    "test": "../../node_modules/.bin/ava",
    "test:ci": "../../scripts/run_package_ci_unit.sh",
    "test:coverage": "../../node_modules/.bin/nyc npm test",
    "prepare": "npm run tsc",
    "tsc": "../../node_modules/.bin/tsc",
    "tsc:listEmittedFiles": "../../node_modules/.bin/tsc --listEmittedFiles",
    "coverage": "python ../../scripts/coverage_handler/coverage.py"
  },
  "ava": {
    "timeout": "15m",
    "failFast": true
  },
  "keywords": [
    "CUMULUS"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/nasa/cumulus/tree/master/packages/checksum/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus"
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "cksum": "^1.3.0"
  }
}
