{
  "name": "@cumulus/es-client",
  "version": "19.0.0",
  "description": "Utilities for working with Elasticsearch",
  "keywords": [
    "CUMULUS",
    "NASA"
  ],
  "engines": {
    "node": ">=20.12.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/nasa/cumulus/tree/master/packages/es-client/README.md",
