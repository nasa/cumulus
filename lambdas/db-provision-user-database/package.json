{
  "name": "@cumulus/db-provision-user-database-lambda",
  "version": "20.1.1",
  "description": "A Lambda function used for provisioning user databases",
  "engines": {
    "node": ">=20.12.2"
  },
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "package": "npm run clean && npm run tsc && npm run webpack && (cd dist/webpack && zip lambda.zip index.js)",
    "test": "../../node_modules/.bin/ava",
    "test:coverage": "../../node_modules/.bin/nyc npm test",
    "test:ci": "../../scripts/run_package_ci_unit.sh",
    "python-lint": "true",
    "prepare": "npm run tsc",
    "tsc": "../../node_modules/.bin/tsc",
    "tsc:listEmittedFiles": "../../node_modules/.bin/tsc --listEmittedFiles",
    "webpack": "../../node_modules/.bin/webpack",
    "coverage": "python ../../scripts/coverage_handler/coverage.py"
  },
  "directories": {
    "test": "tests"
  },
  "license": "Apache-2.0",
  "ava": {
    "timeout": "2m",
    "failFast": true
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.621.0",
    "@cumulus/common": "20.1.1",
    "@cumulus/db": "20.1.1",
    "knex": "2.4.1",
    "pg": "~8.13"
  },
  "devDependencies": {
    "@types/pg": "^7.14.4"
  }
}
